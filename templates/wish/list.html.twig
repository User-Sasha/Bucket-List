{% extends 'base.html.twig' %}

{% block body %}
    <section class="wishes-list">
        <h2>Tous les souhaits</h2>
        {% for wish in wishes %}
            <article>
                <a href="{{ path('wish_Detail', {'wish': wish.id}) }}">{{ wish.title }}</a>
                <a href="{{ path('wish_supprimer', {'id': wish.id}) }}">Supprimer</a>
            </article>
        {% endfor %}
    </section>
{% endblock %}

